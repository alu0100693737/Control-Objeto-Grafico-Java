package controlBola;

public class Bola {
	private final int RADIO = 50;
	private final int POS_INICIO_X = 275;
	private final int POS_INICIO_Y = 175;
	private int diffXInicio;
	private int diffYInicio;
	
	private static int velocidad;
	
	public Bola(int vel) {
		diffXInicio = 0;
		diffYInicio = 0;
		velocidad = vel;
	}

}
